import { createSlice } from "@reduxjs/toolkit";

const initialState = {
  data: null,
};

const configExpense = createSlice({
  name: "configExpense",
  initialState,
  reducers: {
    setConfigExpense: (state, action) => {
      state.data = action.payload;
    },
  },
});

export const { setConfigExpense } = configExpense.actions;

export default configExpense.reducer;
